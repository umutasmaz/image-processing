      "source": [
        "from PIL import Image\n",
        "Img1 = Image.open (\"C:/Users/Monster/Desktop/apple.jpeg\")\n",
        "h,w = Img1.size\n",
        "img = Img1.load()\n",
        "for i in range(0,h):\n",
        "    for j in range(0,w):\n",
        "        R,G,B = img[i,j]\n",
        "        R = 255 - R\n",
        "        G = 255 - G\n",
        "        B = 255 - B\n",
        "        img[i,j] = (R,G,B)\n",
        "Img1.show()"
      ]
    }
  ]
